{
	"plugins": [
		"composer",
		{
			"name": "sync",
			"filter": [
				"**/*",
				"!.github/template-sync/**",
				"!cli/**",
				"!docs/**",
				"!scripts/**",
				"!src/**",
				"!resources/**",
				"!tests/**",
				"!bootstrap.php",
				"!_ide_custom_helpers.php",
				"!testbench.yaml",
				"!phpstan.neon",
				"!phpstan*.neon",
				"!README.md",
				"!CHANGELOG.md",
				"!docker-compose.yml",
				"!docker-compose.*.yml"
			]
		}
	]
}
